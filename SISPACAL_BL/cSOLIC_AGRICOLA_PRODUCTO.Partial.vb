    <System.ComponentModel.DataObjectMethodAttribute(System.ComponentModel.DataObjectMethodType.Update, False)> _
    Public Function ActualizarSOLIC_AGRICOLA_PRODUCTO(ByVal aEntidad As SOLIC_AGRICOLA_PRODUCTO) As Integer
        Try
            Return Me.ActualizarSOLIC_AGRICOLA_PRODUCTO(aEntidad, EL.TipoConcurrencia.Pesimistica)
