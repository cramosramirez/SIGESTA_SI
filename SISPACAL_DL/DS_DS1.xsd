" UserGetMethodName="GetPorCatorcena" UserSourceName="FillPorCatorcena">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT ID_ORDEN_COMBUS, FECHA_EMISION, NOMBRE_PRODUCTO, NO_FACTURA_CCF, FECHA_DESPACHO, CANTIDAD, PRECIO_VENTA, TOTAL, TOTAL_ORDEN,
 TIPO_PROVEEDOR, CODIGO, CODIGO_SINFTO, PROVEEDOR, ID_TIPO_PROVEEDOR, CATORCENA, PLACA, ID_CATORCENA
FROM
(
	SELECT E.ID_ORDEN_COMBUS, E.FECHA_EMISION, 
	CASE E.ESTADO 
	 WHEN 'A' THEN 'ANULADO'
	 ELSE P.NOMBRE_PRODUCTO
	END AS NOMBRE_PRODUCTO, 
	E.NO_FACTURA_CCF, E.FECHA_DESPACHO, D.CANTIDAD, D.PRECIO_VENTA, E.TOTAL, D.CANTIDAD * D.PRECIO_VENTA AS TOTAL_ORDEN,
	CASE 
	WHEN E.ID_TIPO_PROVEEDOR = 1 THEN (SELECT T.NOMBRE_TIPO_PROVEEDOR FROM TIPO_PROVEEDOR T WHERE T.ID_TIPO_PROVEEDOR = E.ID_TIPO_PROVEEDOR)
	WHEN E.ID_TIPO_PROVEEDOR = 2 AND E.CODIGO = '300' THEN 'INJIBOA'
	ELSE (SELECT T.NOMBRE_TIPO_PROVEEDOR FROM TIPO_PROVEEDOR T WHERE T.ID_TIPO_PROVEEDOR = E.ID_TIPO_PROVEEDOR)
	END AS TIPO_PROVEEDOR,	
	CASE E.ID_TIPO_PROVEEDOR
		 WHEN 1 THEN CAST(dbo.QuitarFormatoCODIPROVEE(LEFT(E.CODIGO,6)) AS INT)
		 WHEN 2 THEN CAST(E.CODIGO AS INT)
	END AS CODIGO,
	CASE E.ID_TIPO_PROVEEDOR		
		 WHEN 1 THEN dbo.QuitarFormatoCODIPROVEEDOR2(E.CODIGO) 		 
		 WHEN 2 THEN E.CODIGO
	END AS CODIGO_SINFTO,
	CASE E.ID_TIPO_PROVEEDOR
		WHEN 1 THEN (SELECT ISNULL(RTRIM(P.NOMBRES),'') + ' ' + ISNULL(RTRIM(P.APELLIDOS),'') FROM PROVEEDOR P WHERE P.CODIPROVEEDOR = E.CODIGO)
		WHEN 2 THEN (SELECT ISNULL(T.NOMBRES,'') + ' ' + ISNULL(T.APELLIDOS,'') FROM TRANSPORTISTA T WHERE T.CODTRANSPORT = E.CODIGO)	
	END AS PROVEEDOR, E.ID_TIPO_PROVEEDOR,
	P.ID_PRODUCTO, E.PLACA, (SELECT CZ.CATORCENA FROM CATORCENA_ZAFRA CZ WHERE E.ID_CATORCENA = CZ.ID_CATORCENA) AS CATORCENA, E.ID_CATORCENA
	FROM ORDEN_COMBUSTIBLE E 
	LEFT OUTER JOIN ORDEN_COMBUSTIBLE_PROD D ON E.ID_ORDEN_COMBUS = D.ID_ORDEN_COMBUS
	LEFT OUTER JOIN PRODUCTO_COMBUSTIBLE P ON D.ID_PRODUCTO = P.ID_PRODUCTO
) T
WHERE ID_CATORCENA = @ID_CATORCENA 
AND ((NOT FECHA_DESPACHO IS NULL AND @CON_FACTURA = 1) OR @CON_FACTURA = 0)
ORDER BY ID_ORDEN_COMBUS</CommandText>
