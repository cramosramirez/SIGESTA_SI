            <MainSource>
              <DbSource ConnectionRef="cn (MySettings)" DbObjectName="SELECT E.CODIPROVEEDOR, P.CODIPROVEE, P.CODISOCIO, RTRIM(P.NOMBRES) + ' ' + RTRIM(P.APELLIDOS) AS NOMBRE_PROVEEDOR, SUM(B.NETOTONEL) AS NETOTONEL_CATORCENA, SUM(A.RENDIA_CALC2 * B.NETOTONEL) AS SUM_TONEL_X_REND_CALC2_CATOR FROM ENVIO E, BASCULA B, ANALISIS A, PROVEEDOR P, (SELECT DISTINCT D.ID_ZAFRA, E.CATORCENA, D.FECHA FROM ENVIO E, DIA_ZAFRA D WHERE E.CERRADO = 1 AND E.ID_ZAFRA = D.ID_ZAFRA AND E.DIAZAFRA = D.DIAZAFRA AND D.FECHA = CONVERT(DATETIME,@FECHA_CORTE,103)) F WHERE E.ID_ENVIO = B.ID_ENVIO AND E.ID_ENVIO = A.ID_ENVIO AND E.CODIPROVEEDOR = P.CODIPROVEEDOR AND E.CERRADO = 1 AND E.ID_ZAFRA = F.ID_ZAFRA AND E.CATORCENA = F.CATORCENA AND (SELECT D.FECHA FROM DIA_ZAFRA D WHERE D.ID_ZAFRA = E.ID_ZAFRA AND D.DIAZAFRA = E.DIAZAFRA) &lt;= F.FECHA GROUP BY E.CODIPROVEEDOR, P.CODIPROVEE, P.CODISOCIO, P.NOMBRES, P.APELLIDOS" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="FillPorFECHA_CORTE" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetPorFECHA_CORTE" GeneratorSourceName="FillPorFECHA_CORTE" GetMethodModifier="Public" GetMethodName="GetPorFECHA_CORTE" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetPorFECHA_CORTE" UserSourceName="FillPorFECHA_CORTE">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT
CAST(dbo.QuitarFormatoCODIPROVEE(A.CODIPROVEE) AS INT) AS CODIPROVEE,
CAST(dbo.QuitarFormatoCODISOCIO(A.CODISOCIO) AS INT) AS CODISOCIO,
(SELECT D.DIAZAFRA FROM DIA_ZAFRA D WHERE D.FECHA = CONVERT(DATETIME,@FECHA_CORTE,103)) AS DIAZAFRA,
CONVERT(DATETIME,@FECHA_CORTE,103) AS FECHA_CORTE,
ISNULL(B.RENDIA_CALC2,0) AS RENDIA_CALC2, ISNULL(B.NETOTONEL,0) AS NETOTONEL, ISNULL(B.AZUCAR_CALC2,0) AS AZUCAR_CALC2,
A.NETOTONEL_CATORCENA,
ROUND(A.SUM_TONEL_X_REND_CALC2_CATOR/A.NETOTONEL_CATORCENA,2) AS REND_POND_CATORCENA,
A.NOMBRE_PROVEEDOR,
(SELECT Z.NOMBRE_ZAFRA  FROM ZAFRA Z, DIA_ZAFRA D WHERE D.FECHA = CONVERT(DATETIME,@FECHA_CORTE,103) AND D.ID_ZAFRA = Z.ID_ZAFRA) AS NOMBRE_ZAFRA
FROM
(
	SELECT E.CODIPROVEEDOR, P.CODIPROVEE, P.CODISOCIO, RTRIM(P.NOMBRES) + ' ' + RTRIM(P.APELLIDOS)  AS NOMBRE_PROVEEDOR,
	SUM(B.NETOTONEL) AS NETOTONEL_CATORCENA, SUM(A.RENDIA_CALC2 * B.NETOTONEL) AS SUM_TONEL_X_REND_CALC2_CATOR
	FROM ENVIO E, BASCULA B, ANALISIS A, PROVEEDOR P, 
		 (SELECT DISTINCT D.ID_ZAFRA, E.CATORCENA, D.FECHA FROM ENVIO E, DIA_ZAFRA D WHERE E.CERRADO = 1 AND E.ID_ZAFRA = D.ID_ZAFRA AND E.DIAZAFRA = D.DIAZAFRA AND D.FECHA = CONVERT(DATETIME,@FECHA_CORTE,103)) F
	WHERE E.ID_ENVIO = B.ID_ENVIO
	AND E.ID_ENVIO = A.ID_ENVIO	
	AND E.CODIPROVEEDOR = P.CODIPROVEEDOR
	AND E.CERRADO = 1	
	AND E.ID_ZAFRA = F.ID_ZAFRA
	AND E.CATORCENA = F.CATORCENA 
	AND (SELECT D.FECHA FROM DIA_ZAFRA D WHERE D.ID_ZAFRA = E.ID_ZAFRA AND D.DIAZAFRA = E.DIAZAFRA) &lt;= F.FECHA 
	GROUP BY E.CODIPROVEEDOR, P.CODIPROVEE, P.CODISOCIO, P.NOMBRES, P.APELLIDOS
) A LEFT OUTER JOIN
(	SELECT
	CODIPROVEEDOR,
	dbo.QuitarFormatoCODIPROVEE(CODIPROVEE) AS CODIPROVEE,
	dbo.QuitarFormatoCODISOCIO(CODISOCIO) AS CODISOCIO, DIAZAFRA,
	FECHA_CORTE,
	ROUND(SUM(AZUCAR_CALC2)/SUM(NETOTONEL),2) AS RENDIA_CALC2,
	SUM(NETOTONEL) AS NETOTONEL,
	SUM(AZUCAR_CALC2) AS AZUCAR_CALC2,
	NOMBRE_ZAFRA
	FROM V_MOV_DIARIO
	WHERE FECHA_CORTE = CONVERT(DATETIME,@FECHA_CORTE,103)
	GROUP BY CODIPROVEE, CODISOCIO, DIAZAFRA, FECHA_CORTE, ID_ZAFRA, NOMBRES_PROVEEDOR,
	APELLIDOS_PROVEEDOR, NOMBRE_ZAFRA, FECHA_CORTE, CODIPROVEEDOR, CATORCENA
) B
ON A.CODIPROVEEDOR = B.CODIPROVEEDOR</CommandText>
