                    <CommandText>SELECT C.CATORCENA, 
CASE 
	WHEN P.ES_CONTRIBUYENTE = 1 THEN ' CONTRIBUYENTES' 
	ELSE ' NO CONTRIBUYENTES'
END AS NOMBRE_PLANILLA, 
CAST(dbo.QuitarFormatoCODIPROVEEDOR(P.CODIPROVEEDOR_TRANSPORTISTA) AS INT) AS CODIGO_FORMATEADO,
