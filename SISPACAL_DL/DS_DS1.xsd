                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
	CODIPROVEE, NOMBRE_PROVEEDOR, CODICONTRATO, @ANIOZAFRA AS ANIOZAFRA,
	@DIAZAFRA AS CORTE,
	CODILOTE, NOMBLOTE, EDAD_LOTE, AREA, TONELADAS, TC_MZ, 
	REND, VIP, ZONA,
	TONEL_TC, ROUND(TONEL_TC * REND,2) AS LBS_TC, ROUND(TONEL_TC * REND * VIP,2) AS PAGO_TC,
	TONEL_ENTREGADAS, ROUND(TONEL_ENTREGADAS * REND,2) AS LBS_ENTREGADAS, ROUND(TONEL_ENTREGADAS * REND * VIP,2) AS PAGO_ENTREGADO,
	IIF( (TONEL_TC - TONEL_ENTREGADAS) &lt; 0 , 0, (TONEL_TC - TONEL_ENTREGADAS)) AS TONEL_SALDO, 
	(ROUND(TONEL_TC * REND,2) - ROUND(TONEL_ENTREGADAS * REND,2)) AS LBS_SALDO, 
	(ROUND(TONEL_TC * REND * VIP,2) - ROUND(TONEL_ENTREGADAS * REND * VIP,2)) AS PAGO_SALDO
FROM
(
	SELECT CAST(dbo.QuitarFormatoCODIPROVEE(P.CODIPROVEE) AS INT) AS CODIPROVEE,
	RTRIM(ISNULL(P.NOMBRES,'')) + ' ' + RTRIM(ISNULL(P.APELLIDOS,'')) AS NOMBRE_PROVEEDOR,
	CAST(dbo.QuitarFormatoCODICONTRATO(C.CODICONTRATO) AS INT) AS CODICONTRATO, 
	L.CODILOTE, L.NOMBLOTE, L.EDAD_LOTE, LC.MZ_CENSO AS AREA, LC.TONEL_MZ_CENSO AS TONELADAS, LC.TONEL_MZ_CENSO AS TC_MZ,  LC.TONEL_CENSO AS TONEL_TC, 
	ISNULL((SELECT SUM(B.NETOTONEL) FROM ENVIO E, BASCULA B 
		WHERE E.ID_ENVIO = B.ID_ENVIO AND E.ACCESLOTE = L.ACCESLOTE AND E.ID_ZAFRA = @ID_ZAFRA AND E.CERRADO = 1 AND E.DIAZAFRA BETWEEN 1 AND @DIAZAFRA),0) AS TONEL_ENTREGADAS,	
	CASE 
		WHEN EXISTS(SELECT 1 FROM ENVIO E WHERE E.ACCESLOTE = L.ACCESLOTE AND E.ID_ZAFRA = @ID_ZAFRA AND E.CERRADO = 1) 
		  THEN (SELECT ROUND(SUM(A.AZUCAR_CATORCENA_REAL)/SUM(B.NETOTONEL),4) FROM ENVIO E, ANALISIS A, BASCULA B WHERE E.ACCESLOTE = L.ACCESLOTE AND E.ID_ENVIO = A.ID_ENVIO AND E.ID_ENVIO = B.ID_ENVIO AND E.CERRADO = 1 AND E.ID_ZAFRA = @ID_ZAFRA)
		ELSE (SELECT ROUND(SUM(A.AZUCAR_CATORCENA_REAL)/SUM(B.NETOTONEL),4) FROM ENVIO E, ANALISIS A, BASCULA B WHERE E.ID_ENVIO = A.ID_ENVIO AND E.ID_ENVIO = B.ID_ENVIO AND E.CERRADO = 1 AND E.ID_ZAFRA = @ID_ZAFRA)
	END AS REND,
	(SELECT Z.PRECIO_LIBRA_AZUCAR FROM ZAFRA Z WHERE Z.ID_ZAFRA = @ID_ZAFRA) AS VIP, 
	(SELECT Z.DESCRIP_ZONA FROM ZONAS Z WHERE Z.ZONA = L.ZONA) AS ZONA
	FROM LOTES L, LOTES_COSECHA LC, CONTRATO C, PROVEEDOR P
	WHERE L.CODICONTRATO = C.CODICONTRATO 
	AND C.CODIPROVEEDOR = P.CODIPROVEEDOR 	
	AND L.ACCESLOTE = LC.ACCESLOTE
	AND LC.ID_ZAFRA = @ID_ZAFRA
	AND (dbo.QuitarFormatoCODIPROVEE(P.CODIPROVEE) = @CODIPROVEE OR @CODIPROVEE = '' )
	--AND EXISTS(SELECT 1 FROM ENVIO E WHERE E.ACCESLOTE = L.ACCESLOTE AND E.ID_ZAFRA = @ID_ZAFRA AND E.DIAZAFRA BETWEEN 1 AND @DIAZAFRA AND E.CERRADO = 1)
) T</CommandText>
