            <MainSource>
              <DbSource ConnectionRef="cn (MySettings)" DbObjectName="SELECT E.ID_ZAFRA,(SELECT Z.NOMBRE_ZAFRA FROM ZAFRA Z WHERE Z.ID_ZAFRA = E.ID_ZAFRA) AS NOMBRE_ZAFRA, E.ID_ENVIO,E.DIAZAFRA,E.CATORCENA, (SELECT D.FECHA FROM DIA_ZAFRA D WHERE D.ID_ZAFRA = E.ID_ZAFRA AND D.DIAZAFRA = E.DIAZAFRA) AS FECHA_CORTE, E.COMPTENVIO, E.NROBOLETA, dbo.QuitarFormatoCODICONTRATO(L.CODICONTRATO) AS CODICONTRATO, CAST(P.CODIPROVEEDOR AS INT) AS CODIPROVEEDOR, dbo.QuitarFormatoCODIPROVEE(P.CODIPROVEE) AS CODIPROVEE, dbo.QuitarFormatoCODISOCIO(P.CODISOCIO) AS CODISOCIO, RTRIM(P.NOMBRES) + ' ' + RTRIM(P.APELLIDOS) AS NOMBRES_PROVEEDOR, L.ACCESLOTE,L.CODILOTE,L.NOMBLOTE,B.NETOTONEL, dbo.QuitarFormatoCODIAGRON(LC.CODIAGRON) AS CODIAGRON, RTRIM(G.NOMBRE_AGRONOMO) + ' ' + RTRIM(G.APELLIDO_AGRONOMO) AS NOMBRE_AGRONOMO,E.FECHA_QUEMA,B.FECHA_LEE_TARA, E.HORAS_QUEMA, (SELECT V.NOM_VARIEDAD FROM VARIEDAD V WHERE V.CODIVARIEDA = L.CODIVARIEDA) AS NOM_VARIEDAD, A.BRIX,A.POL,A.POL_JUGO,A.POL_CANA,A.RENDIA_CALC2,A.RENDIA_REAL,A.PH,A.HUMEDAD,A.AZUCAR_REDUCTORES AS REDUCTORES, CASE WHEN NOT E.FECHA_QUEMA IS NULL THEN B.NETOTONEL*E.HORAS_QUEMA ELSE 0 END AS TON_HORAS_QUEMA, ISNULL((B.NETOTONEL*A.BRIX),0) AS TON_BRIX, ISNULL((B.NETOTONEL*A.POL_CANA),0) AS TON_POL_CANA, ISNULL((B.NETOTONEL*A.RENDIA_CALC2),0) AS TON_REND2, ISNULL((B.NETOTONEL*A.RENDIA_REAL),0) AS TON_RENDREAL, ISNULL((B.NETOTONEL*A.PH),0) AS TON_PH, ISNULL((B.NETOTONEL*A.HUMEDAD),0) AS TON_HUMEDAD, ISNULL((B.NETOTONEL*A.AZUCAR_REDUCTORES),0) AS TON_REDUCTORES, ISNULL((B.NETOTONEL*A.POL_JUGO),0) AS TON_POL_JUGO, E.NUMRECIBO_CANA, ISNULL((B.NETOTONEL*A.POL),0) AS TON_POL, A.AZUCAR_CALC2, A.BAGAZO_BRUTO, A.BAGAZO_NETO, ISNULL((B.NETOTONEL*A.BAGAZO_BRUTO),0) AS TON_BAGAZO_BRUTO, ISNULL((B.NETOTONEL*A.BAGAZO_NETO),0) AS TON_BAGAZO_NETO, (SELECT DL.DEXTRANA FROM DATOS_LABORATORIO DL WHERE DL.ID_ANALISIS = A.ID_ANALISIS) AS DEXTRANA, ISNULL((B.NETOTONEL*(SELECT DL.DEXTRANA FROM DATOS_LABORATORIO DL WHERE DL.ID_ANALISIS = A.ID_ANALISIS)),0) AS TON_DEXTRANA, CASE WHEN NOT (SELECT DL.DEXTRANA FROM DATOS_LABORATORIO DL WHERE DL.ID_ANALISIS = A.ID_ANALISIS) IS NULL THEN B.NETOTONEL ELSE 0 END AS TON_SOLO_DEXTRA FROM ENVIO E, ANALISIS A, BASCULA B, LOTES L, LOTES_COSECHA LC, PROVEEDOR P, AGRONOMO G WHERE E.ID_ENVIO = A.ID_ENVIO AND E.ID_ENVIO = B.ID_ENVIO AND E.ACCESLOTE = L.ACCESLOTE AND L.ACCESLOTE = LC.ACCESLOTE AND E.ID_ZAFRA = LC.ID_ZAFRA AND L.CODIPROVEEDOR = P.CODIPROVEEDOR AND LC.CODIAGRON = G.CODIAGRON /*AND dbo.QuitarFormatoCODIAGRON(LC.CODIAGRON) = @CODIAGRON*/ AND (SELECT D.FECHA FROM DIA_ZAFRA D WHERE D.ID_ZAFRA = E.ID_ZAFRA AND D.DIAZAFRA = E.DIAZAFRA) = @FECHA_CORTE" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="FillPorDiaZafra" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetPorDiaZafra" GeneratorSourceName="FillPorDiaZafra" GetMethodModifier="Public" GetMethodName="GetPorDiaZafra" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetPorDiaZafra" UserSourceName="FillPorDiaZafra">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT T.*,
	(select SUM(a.azucar_calc2)/SUM(b.netotonel) from ENVIO e, BASCULA b, ANALISIS a where e.ID_ENVIO = a.ID_ENVIO and e.ID_ENVIO = b.ID_ENVIO and e.ID_ZAFRA = T.ID_ZAFRA AND e.DIAZAFRA = T.DIAZAFRA) as REND_POND2,
	(select SUM(b.netotonel*a.brix)/SUM(b.netotonel) from ENVIO e, BASCULA b, ANALISIS a where e.ID_ENVIO = a.ID_ENVIO and e.ID_ENVIO = b.ID_ENVIO and e.ID_ZAFRA = T.ID_ZAFRA AND e.DIAZAFRA = T.DIAZAFRA) as BRIX_POND,
	(select SUM(b.netotonel*a.pol_cana)/SUM(b.netotonel) from ENVIO e, BASCULA b, ANALISIS a where e.ID_ENVIO = a.ID_ENVIO and e.ID_ENVIO = b.ID_ENVIO and e.ID_ZAFRA = T.ID_ZAFRA AND e.DIAZAFRA = T.DIAZAFRA) as POLCANA_POND,
	(select SUM(b.netotonel*a.humedad)/SUM(b.netotonel) from ENVIO e, BASCULA b, ANALISIS a where e.ID_ENVIO = a.ID_ENVIO and e.ID_ENVIO = b.ID_ENVIO and e.ID_ZAFRA = T.ID_ZAFRA AND e.DIAZAFRA = T.DIAZAFRA) as HUMEDAD_POND,
	(select SUM(b.netotonel*a.azucar_reductores)/SUM(b.netotonel) from ENVIO e, BASCULA b, ANALISIS a where e.ID_ENVIO = a.ID_ENVIO and e.ID_ENVIO = b.ID_ENVIO and e.ID_ZAFRA = T.ID_ZAFRA AND e.DIAZAFRA = T.DIAZAFRA) as REDUCTORES_POND		
FROM (
	SELECT 
	E.ID_ZAFRA,(SELECT Z.NOMBRE_ZAFRA FROM ZAFRA Z WHERE Z.ID_ZAFRA = E.ID_ZAFRA) AS NOMBRE_ZAFRA,
	E.ID_ENVIO,E.DIAZAFRA,E.CATORCENA, 
	(SELECT D.FECHA FROM DIA_ZAFRA D WHERE D.ID_ZAFRA = E.ID_ZAFRA AND D.DIAZAFRA = E.DIAZAFRA) AS FECHA_CORTE, 
	E.COMPTENVIO, E.NROBOLETA,
	dbo.QuitarFormatoCODICONTRATO(L.CODICONTRATO) AS CODICONTRATO,
	CAST(P.CODIPROVEEDOR AS INT) AS CODIPROVEEDOR,
	dbo.QuitarFormatoCODIPROVEE(P.CODIPROVEE) AS CODIPROVEE,
	dbo.QuitarFormatoCODISOCIO(P.CODISOCIO) AS CODISOCIO,
	RTRIM(P.NOMBRES) + ' ' + RTRIM(P.APELLIDOS) AS NOMBRES_PROVEEDOR,
	L.ACCESLOTE,L.CODILOTE,L.NOMBLOTE,B.NETOTONEL,
	dbo.QuitarFormatoCODIAGRON(LC.CODIAGRON) AS CODIAGRON,
	RTRIM(G.NOMBRE_AGRONOMO) + ' ' + RTRIM(G.APELLIDO_AGRONOMO) AS NOMBRE_AGRONOMO,E.FECHA_QUEMA,B.FECHA_LEE_TARA,
	E.HORAS_QUEMA,
	(SELECT V.NOM_VARIEDAD FROM VARIEDAD V WHERE V.CODIVARIEDA = L.CODIVARIEDA) AS NOM_VARIEDAD,
	A.BRIX,A.POL,A.POL_JUGO,A.POL_CANA,A.RENDIA_CALC2,A.RENDIA_REAL,A.PH,A.HUMEDAD,A.AZUCAR_REDUCTORES AS REDUCTORES,
	CASE
	WHEN NOT E.FECHA_QUEMA IS NULL THEN B.NETOTONEL*E.HORAS_QUEMA 
	ELSE 0
	END AS TON_HORAS_QUEMA,
	ISNULL((B.NETOTONEL*A.BRIX),0) AS TON_BRIX,
	ISNULL((B.NETOTONEL*A.POL_CANA),0) AS TON_POL_CANA,
	ISNULL((B.NETOTONEL*A.RENDIA_CALC2),0) AS TON_REND2,
	ISNULL((B.NETOTONEL*A.RENDIA_REAL),0) AS TON_RENDREAL,
	ISNULL((B.NETOTONEL*A.PH),0) AS TON_PH,
	ISNULL((B.NETOTONEL*A.HUMEDAD),0) AS TON_HUMEDAD,
	ISNULL((B.NETOTONEL*A.AZUCAR_REDUCTORES),0) AS TON_REDUCTORES,
	ISNULL((B.NETOTONEL*A.POL_JUGO),0) AS TON_POL_JUGO, E.NUMRECIBO_CANA,
	ISNULL((B.NETOTONEL*A.POL),0) AS TON_POL, A.AZUCAR_CALC2,	
	A.BAGAZO_BRUTO, A.BAGAZO_NETO,
	ISNULL((B.NETOTONEL*A.BAGAZO_BRUTO),0) AS TON_BAGAZO_BRUTO,
	ISNULL((B.NETOTONEL*A.BAGAZO_NETO),0) AS TON_BAGAZO_NETO,
	(SELECT DL.DEXTRANA FROM DATOS_LABORATORIO DL WHERE DL.ID_ANALISIS = A.ID_ANALISIS) AS DEXTRANA, 
	ISNULL((B.NETOTONEL*(SELECT DL.DEXTRANA FROM DATOS_LABORATORIO DL WHERE DL.ID_ANALISIS = A.ID_ANALISIS)),0) AS TON_DEXTRANA,
	CASE 
	WHEN NOT (SELECT DL.DEXTRANA FROM DATOS_LABORATORIO DL WHERE DL.ID_ANALISIS = A.ID_ANALISIS) IS NULL THEN B.NETOTONEL 
	ELSE 0
	END AS TON_SOLO_DEXTRA
	FROM ENVIO E, ANALISIS A, BASCULA B, LOTES L, LOTES_COSECHA LC, PROVEEDOR P, AGRONOMO G
	WHERE E.ID_ENVIO = A.ID_ENVIO
	AND E.ID_ENVIO = B.ID_ENVIO
	AND E.ACCESLOTE = L.ACCESLOTE
	AND L.ACCESLOTE = LC.ACCESLOTE
	AND E.ID_ZAFRA = LC.ID_ZAFRA
	AND L.CODIPROVEEDOR = P.CODIPROVEEDOR
	AND LC.CODIAGRON = G.CODIAGRON
	--AND dbo.QuitarFormatoCODIAGRON(LC.CODIAGRON) = @CODIAGRON
	AND (SELECT D.FECHA FROM DIA_ZAFRA D WHERE D.ID_ZAFRA = E.ID_ZAFRA AND D.DIAZAFRA = E.DIAZAFRA) = @FECHA_CORTE
) T</CommandText>
