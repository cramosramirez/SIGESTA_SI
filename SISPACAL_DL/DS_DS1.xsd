FROM ORDEN_COMBUSTIBLE AS C INNER JOIN
PROVEEDOR_COMBUSTIBLE AS P ON C.ID_PROVEEDOR_COMBUS = P.ID_PROVEEDOR_COMBUS INNER JOIN
ZAFRA AS Z ON C.ID_ZAFRA = Z.ID_ZAFRA INNER JOIN
USUARIO AS U ON C.USUARIO_CREA = U.USUARIO INNER JOIN
ORDEN_COMBUSTIBLE_PROD OC ON C.ID_ORDEN_COMBUS = OC.ID_ORDEN_COMBUS INNER JOIN
PRODUCTO_COMBUSTIBLE PROD ON OC.ID_PRODUCTO = PROD.ID_PRODUCTO
