          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="RECIBO_CANATableAdapter" GeneratorDataComponentClassName="RECIBO_CANATableAdapter" Name="RECIBO_CANA" UserDataComponentName="RECIBO_CANATableAdapter">
            <MainSource>
              <DbSource ConnectionRef="cn (MySettings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="FillPorENVIO" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetPorENVIO" GeneratorSourceName="FillPorENVIO" GetMethodModifier="Public" GetMethodName="GetPorENVIO" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetPorENVIO" UserSourceName="FillPorENVIO">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT  dbo.QuitarFormatoCODIPROVEEDOR(E.CODIPROVEEDOR) AS CODIPROVEEDOR, RTRIM(ISNULL(P.NOMBRES,'')) + ' ' + RTRIM(ISNULL(P.APELLIDOS,'')) AS PROVEEDOR, 
E.CODTRANSPORT, 
RTRIM(ISNULL(T.NOMBRES,'')) + ' ' + RTRIM(ISNULL(T.APELLIDOS,'')) AS TRANSPORTISTA, E.PLACAVEHIC, 
NOMBRES_MOTORISTA  AS MOTORISTA, 
(SELECT D.NOMBRE_DEPTO FROM DEPARTAMENTO D WHERE ML.CODI_DEPTO = D.CODI_DEPTO) DEPARTAMENTO, 
(SELECT D.NOMBRE_DEPTO FROM DEPARTAMENTO D WHERE ML.CODI_DEPTO = D.CODI_DEPTO) + '/' + 
(SELECT M.NOMBRE_MUNI FROM MUNICIPIO M WHERE ML.CODI_DEPTO = M.CODI_DEPTO AND ML.CODI_MUNI = M.CODI_MUNI) + '/' + 
(SELECT C.NOMBRE_CANTON FROM CANTON C WHERE ML.CODI_DEPTO = C.CODI_DEPTO AND ML.CODI_MUNI = C.CODI_MUNI AND ML.CODI_CANTON = C.CODI_CANTON) AS DEPTO_MUNI_CANTON, L.CODILOTE, L.NOMBLOTE, 
(SELECT V.NOM_VARIEDAD FROM VARIEDAD V WHERE V.CODIVARIEDA = L.CODIVARIEDA) NOM_VARIEDAD, 
E.ID_CARGADORA, L.ZONA, E.COMPTENVIO, 
(SELECT ML.CODI_DEPTO + ML.CODI_MUNI + ML.CODI_CANTON FROM MAESTRO_LOTES ML WHERE ML.ID_MAESTRO = L.ID_MAESTRO) AS CODITARIFA, 
L.EDAD_LOTE, E.ID_TIPO_CANA, FECHA_QUEMA, FECHA_CORTA, FECHA_CARGA, FECHA_PATIO, FECHA_LEE_BRUTO, FECHA_LEE_TARA, 
dbo.QuitarFormatoCODICONTRATO(L.CODICONTRATO) AS CODICONTRATO, BRUTO, TARA, NETOLIBRAS, NETOTONEL, CAST(B.FECHA_LEE_BRUTO AS DATE) AS FECHA_ENTRADA, E.NROBOLETA, E.ID_TIPOTRANS, 
(SELECT U.NOMBRE FROM USUARIO U WHERE U.USUARIO = B.USUARIO_LEE_TARA) AS NOM_USU_LEE_TARA, 
E.NUMRECIBO_CANA, (SELECT C.NOMBRE_CARGADOR FROM CARGADOR C WHERE C.ID_CARGADOR = E.ID_CARGADOR) AS NOMBCARGADOR,
CASE E.TRANSPORTE_PROPIO
	WHEN 1 THEN 'TRANSPORTE PROPIO'
	ELSE ''
END AS TRANSPORTE_PROPIO, 
(SELECT Z.NOMBRE_ZAFRA FROM ZAFRA Z WHERE Z.ID_ZAFRA = E.ID_ZAFRA) AS NOMBRE_ZAFRA,
dbo.QuitarFormatoCODIPROVEE(P.CODIPROVEE) AS CODIPROVEE, dbo.QuitarFormatoCODISOCIO(P.CODISOCIO) AS CODISOCIO,
CASE SERVICIO_ROZA 
WHEN 1 THEN 'ROZA: ' + (SELECT R.NOMBRE_PROVEEDOR_ROZA FROM PROVEEDOR_ROZA R WHERE R.ID_PROVEEDOR_ROZA = E.ID_PROVEEDOR_ROZA)
ELSE ''
END AS PROVEEDOR_ROZA,
CASE 
	WHEN EXISTS(SELECT 1 FROM CONTRATO C WHERE C.CODICONTRATO = L.CODICONTRATO AND C.CODIPROVEEDOR &lt;&gt; L.CODIPROVEEDOR) THEN 
		(SELECT RTRIM(R.NOMBRES) + ' ' + RTRIM(R.APELLIDOS) FROM CONTRATO C, PROVEEDOR R WHERE C.CODICONTRATO = L.CODICONTRATO AND C.CODIPROVEEDOR = R.CODIPROVEEDOR) 
	ELSE ''
END AS COOPERATIVA
FROM  ENVIO E, LOTES L, MAESTRO_LOTES ML, BASCULA B, PROVEEDOR P, TRANSPORTISTA T
WHERE E.ID_ENVIO = @ID_ENVIO 
AND E.ID_ENVIO = B.ID_ENVIO 
AND E.ACCESLOTE = L.ACCESLOTE
AND L.ID_MAESTRO = ML.ID_MAESTRO 
AND L.CODIPROVEEDOR = P.CODIPROVEEDOR
