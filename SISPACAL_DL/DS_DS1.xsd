                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT C.ID_ORDEN_COMBUS, P.NOMBRE_PROVEEDOR_COMBUS, Z.NOMBRE_ZAFRA,
(SELECT ISNULL(RTRIM(T.NOMBRES),'') + ISNULL(RTRIM(T.APELLIDOS),'') FROM TRANSPORTISTA T WHERE T.CODTRANSPORT = C.CODIGO) AS NOMBRE_PROVEEDOR,
