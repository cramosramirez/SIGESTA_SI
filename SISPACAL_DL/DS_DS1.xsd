C.LICENCIA, C.DUI, C.FECHA_EMISION, C.USUARIO_CREA, U.NOMBRE AS USUARIO,
CASE C.ID_TIPO_PROVEEDOR
WHEN 1 THEN RTRIM(dbo.QuitarFormatoCODIPROVEE(LEFT(C.CODIGO,6)) + ' ' + dbo.QuitarFormatoCODISOCIO(RIGHT(C.CODIGO,4)))
ELSE C.CODIGO
